<view class="section-container">
    <view class="rv-row rv-row-center">
        <view class="rv-col">
            <image class="logo" src="../../asset/logo/rovingui-LOGO.png" />
        </view>
    </view>
    <view class="divider" />
    <view class="rv-row  rv-row-center">
        <view class="rv-col">
            RovingUI
        </view>
    </view>
    <!-- Github ribbon -->
    <view class="corner-ribbon rv-bg-deepspace" bindtap="showGithubPage">
        <view class="ribbon-content">
            Star me on Github
        </view>
    </view>
</view>
<view class="section-container">
    基础组件
    <view class="divider" />
    <rv-button bindrvclick="navigateTo" data-url="../button/index">rv-button 按钮</rv-button>
    <view class="divider" />
    <rv-button bindrvclick="navigateTo" data-url="../grid/index">rv-grid 栅格</rv-button>
    <view class="divider" />
    <rv-button bindrvclick="navigateTo" data-url="../helper/index">CSS Helper 样式</rv-button>
    <view class="divider" />
</view>
<view class="section-container">
    表单
    <view class="divider" />
    <view class="divider" />
</view>
<view class="section-container">
    展示
    <view class="divider" />
    <rv-button bindrvclick="navigateTo" data-url="../badge/index">rv-badge 徽标</rv-button>
    <view class="divider" />
</view>
<view class="section-container">
    反馈
    <view class="divider" />
    <rv-button bindrvclick="navigateTo" data-url="../toast/index">rv-toast 通知</rv-button>
    <view class="divider" />
</view>
<view class="section-container">
    导航
    <view class="divider" />
    <rv-button bindrvclick="navigateTo" data-url="../breadcrumb/index">rv-breadcrumb 面包屑</rv-button>
    <view class="divider" />
</view>

<!-- Github pop-up -->
<view class="github-container rv-bg-deepspace {{showGithubPage ? 'github-expand' : 'github-closing'}}">
    <view class="github-mask rv-row rv-row-space-around rv-row-middle">
        <view class="rv-col"></view>
    </view>
    <view class="close-button" bindtap="closeGithubPage">
        <image src="data:image/svg+xml;utf8;base64," />
    </view>
    <view>
        <view class="place-holder" />
        <view class="rv-row rv-row-center">
            <view class="rv-col">
                <image class="logo" src="../../asset/logo/rovingui-LOGO.png" />
            </view>
        </view>
        <view class="rv-divider" />
        <view class="rv-row rv-row-middle rv-row-center">
            <view class="rv-col-20 cover-card">
                <view class="url-box rv-font-16" bindtap="copyUrl">
                    <view class="github-title">
                        Github
                    </view>
                    <view class="rv-divider rv-divider-line" />
                    https://github.com/rovinglight/rovingui-weapp
                </view>
            </view>
        </view>
        <view class="rv-row rv-row-middle rv-row-center">
            <view class="rv-col-18">
                <button class="url-box share-btn rv-font-16" open-type="share">
                    转发给好友
                </button>
            </view>
        </view>
        <view class="place-holder" />
        <view class="rv-row rv-row-middle rv-row-center">
            <view class="rv-col-10">
                <image class="bottom-image" src="data:image/svg+xml;utf8;base64," />
            </view>
        </view>
    </view>
</view>

<rv-toast toastList="{{_rv_toast_list}}" bindtoasthelper="toastHelper" />